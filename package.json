{
  "name": "{{name}}",
  "version": "1.0.0-beta.4",
  "description": "{{description}}",
  "author": "{{author}}",
  "scripts": {
    "bootstrap": "pnpm install",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "staging": "vue-tsc --noEmit && vite build --mode staging",
    "serve": "vite preview --host",
    "prepare": "husky install",
    "commit": "git-cz",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,jsx,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,jsx,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "stylelint": "stylelint **/*.{html,vue,css,sass,scss,less}"
  },
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@vueuse/core": "^7.1.2",
    "ant-design-vue": "^3.0.0-alpha.13",
    "axios": "0.21.1",
    "crypto-js": "^4.1.1",
    "current-device": "0.10.2",
    "dayjs": "^1.10.7",
    "lodash": "4.17.21",
    "lodash-es": "^4.17.21",
    "mitt": "2.1.0",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "pinia": "2.0.0-rc.6",
    "qs": "^6.10.1",
    "resize-observer-polyfill": "^1.5.1",
    "screenfull": "5.1.0",
    "store": "2.0.12",
    "stylelint-scss": "^3.20.1",
    "ua-parser-js": "0.7.28",
    "vue": "^3.2.6",
    "vue-router": "4.0.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jest": "^26.0.22",
    "@types/nprogress": "^0.2.0",
    "@types/store": "^2.0.2",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vitejs/plugin-vue-jsx": "^1.3.0",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/test-utils": "^2.0.0-rc.4",
    "autoprefixer": "^10.4.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.17.0",
    "husky": "^7.0.2",
    "jest": "^26.6.3",
    "less": "^4.1.2",
    "lint-staged": "^11.1.2",
    "postcss": "^8.4.4",
    "postcss-html": "^1.3.0",
    "postcss-less": "^5.0.0",
    "postcss-viewport-height-correction": "^1.1.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "stylelint": "14.1.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.3.2",
    "unplugin-vue-components": "^0.14.9",
    "vite": "^2.5.2",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-style-import": "^1.4.0",
    "vite-plugin-svg-icons": "^1.0.4",
    "vite-plugin-windicss": "^1.5.3",
    "vue-tsc": "^0.3.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": "^12 || >=14"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
